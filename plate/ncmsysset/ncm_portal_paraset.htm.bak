<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=GBK">
<title>Portal设置</title>
    <link rel="stylesheet" type="text/css" href="/newver/resources/css/ext-all.css" />
    <script type="text/javascript" src="/newver/resources/js/ext-all_n.js"></script>
    
  <script>  
    
Ext.require([
    'Ext.form.*',
    'Ext.layout.container.Column',
    'Ext.window.MessageBox',
    'Ext.fx.target.Element',
    'Ext.data.*'
]);


Ext.onReady(function(){
    Ext.define('State', {
        extend: 'Ext.data.Model',
        fields: [
            {type: 'string', name: 'groupid'},
            {type: 'string', name: 'groupname'}
        ]
    });

    var autype = [
        {"groupid":"CHAP","groupname":"CHAP"},
        {"groupid":"PAP","groupname":"PAP"}
    ];
    var store_autype = Ext.create('Ext.data.Store', {
        model: 'State',
        data: autype
    });
    var autypeCombo = Ext.create('Ext.form.field.ComboBox', {
        fieldLabel: '认证方式',
        labelWidth: 140,
        width: 273,
        emptyText : '请选择',
        valueField:'groupid',
        displayField: 'groupname',
        name : 'AuthType',      
        value:'[#AuthType#]',
				//allowBlank: true,
        forceSelection: true,
        store: store_autype,
        queryMode: 'local',
        typeAhead: true
    });
    
    var SeriousInfoGroup = {
        xtype: 'fieldset',
        width: 350,
        title: 'Portal参数设置',
        style:{
		        marginRight: 'auto',
		        marginLeft: 'auto'
				},  
        layout: 'anchor',
        defaults: {
            anchor: '100%',
            height:28,
            labelStyle: 'padding-left:4px;'
        },
        collapsible: true,
        items: [{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[{
		            xtype: 'textfield',
		            width: 273,
		            name: 'PortalName',
		            value: '[#PortalName#]',
		            allowBlank: false,
		            fieldLabel: 'Portal名称'
		        }]
        },{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[{
		            xtype: 'textfield',
            		width: 273,
		            emptyText: ' ',
		            name: 'TcpPort',
		            value: '[#TcpPort#]',
		            fieldLabel: 'TCP端口'
            }]
        },{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[{
		            xtype: 'textfield',
		            width: 273,
		            name: 'MinTcpProcess',
		            value: '[#MinTcpProcess#]',
		            fieldLabel: '最小TCP进程数'
            }]
        },{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[{
		            xtype: 'textfield',
		            width: 273,
		            name: 'MaxTcpProcess',
		            value: '[#MaxTcpProcess#]',
		            fieldLabel: '最大TCP进程数'
            }]
        },{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[{
		            xtype: 'textfield',
		            width: 273,
		            name: 'AdminIp',
		            value: '[#AdminIp#]',
		            fieldLabel: '管理地址'
            }]
        },{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[{
		            xtype: 'textfield',
		            width: 273,
		            name: 'AdminPort',
		            value: '[#AdminPort#]',
		            fieldLabel: '管理端口'
            }]
        },{
            labelWidth: 140,
            layout: 'hbox',
            xtype: 'container',
            items:[
            		autypeCombo
            ]
        },{
            xtype: 'hidden',
            name: 'FunName',
            value:'ncmPortalParaSave_par'
        }]
    };
    
    
    var fp = Ext.create('Ext.FormPanel', {         
        frame: false,
				border:false,
        method:'POST',
        name:'form1',
        fieldDefaults: {
            labelWidth: 140
        },
				width:980,
        renderTo:'corp_edit',
				// style : 'text-align:center',              //SeriousInfoGroup 文字居中
        bodyPadding: 20,
        items: [
            SeriousInfoGroup,
        ],
        buttons: [{
						text: '保存',
						handler: function(){
								if(fp.getForm().isValid()){
										this.disabled=true;
										var guserid=document.getElementById("form1").userid.value;
										// alert(guserid);
										fp.form.doAction('submit',{
												url:'/pronline/Msg',
												method:'POST',
												params:{userid:guserid},
												success:function(form,action){ 
														Ext.Msg.alert('操作',"保存成功");
														document.getElementById("form1").userid.value=action.result.id;
													  // window.opener.reflash();	
												},
												failure:function(form,action){    		 
												  	Ext.Msg.alert('操作',"保存失败");
												}
										})
								this.disabled=false; 
								}
						}
        },{
            text: '重置',
            style : 'margin-right:5px',
            handler: function(){
                fp.getForm().reset();
            }
        },{
            text: '应用',
            style : 'margin-right:370px',
            handler: function(){
                 document.getElementById("form1").userid.value = "app";
                 if(fp.getForm().isValid())
                 {
				             this.disabled=true;
				             var guserid="app";
				             fp.form.doAction('submit',{
						             url:'/pronline/Msg',
						             method:'POST',
						             params:{app:guserid},
						             success:function(form,action){ 
					           					Ext.Msg.alert('操作',"应用成功");
					           					document.getElementById("form1").userid.value=action.result.id;
					 										//	window.opener.reflash();	
						             },
						             failure:function(form,action){    		 
						                    Ext.Msg.alert('操作',"应用失败");
				            		 }
				             })
				        		 this.disabled=false; 
				         }
            }
        }]
    });
    if(document.getElementById("form1").userid.value==''){
       fp.getForm().getValues().groupid=document.getElementById("form1").groupid.value;
    }
});
</script>
</head>
<body >
	  <center><div id="corp_edit"></div></center>
    <div id="joinfac" class="x-hide-display">
         <select name="faccode"><option value="aaaa">fffff</option>/</select>
    </div> 
    <form name="form1" id="form1">
    	<input type="hidden" name="userid" value="[#userid#]">
    	<input type="hidden" name="app" value="">
    	<input type="hidden" name="groupid" value="[#groupid#]">
    </form>
</body>
</html>
