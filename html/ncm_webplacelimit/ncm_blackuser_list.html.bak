var store;
var temp_id='';
var prename='';

function reflash(){
   
   var utype1 = document.getElementById('form1').utype.value;
   var name1 = document.getElementById('form1').name.value;
   var memo1 = document.getElementById('form1').memo.value;
   var validtime1 = document.getElementById('form1').validtime.value;
   var systime1 = document.getElementById('form1').systime.value;
   var keyword1 = document.getElementById('form1').keyword.value;
   var caExport1 = document.getElementById('form1').caExport.value; //导出
   
   store.load({params: {utype:utype1,name:name1,memo:memo1,validtime:validtime1,systime:systime1,keyword:keyword1,caExport:caExport1}});
   
