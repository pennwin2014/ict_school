    function testright(){
				fptest.getForm().doAction('submit',{
				    url:'/pronline/Msg',
				    method:'post',
				    params:{FunName:'ncsTestRight_v4'},
				
				    success:function(form,action){                
				  			timeout_count=0;
				 		    var msgaa=action.result.reason;
				 		    if(msgaa=='license'){
				 		    		if(!wintest){
				 	           		msgaa=action.result.id;
				          			wintest=Ext.create('Ext.Window', {
										       	title: '系统提示',
										       	id:'wintest',     
										 				//header:false,
										        width: 250,
										        x:600,
										        y:210,
										        height: 90,
										        closable: true,
										        modal: false,
										  			//closeAction: 'hide',
										        bodyPadding: 10,
										        html:msgaa
								    		});
				            		wintest.show();
				           	}
				 		    }
				    },
				   	failure:function(form,action){
				   			timeout_count++;
				   		 	if(timeout_count>5) return;
				   		  if(!wintest){
				        		wintest=Ext.create('Ext.Window', {
								        title: '系统提示',
